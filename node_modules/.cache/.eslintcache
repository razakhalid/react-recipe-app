s];\n    const index = newIngredients.findIndex((i) => i.id === id);\n    newIngredients[index] = ingredient;\n    handleChange({ ingredients: newIngredients });\n  }\n\n  function handleIngredientAdd() {\n    const newIngredient = {\n      id: Date.now().toString,\n      name: \"\",\n      amount: \"\",\n    };\n    handleChange({ ingredients: [...recipe.ingredients, newIngredient] });\n  }\n\n  function handleIngredientDelete(id) {\n    handleChange({\n      ingredients: recipe.ingredients.filter((i) => i.id !== id),\n    });\n  }\n\n  return (\n    <>\n      <div className=\"recipe-edit\">\n        <div className=\"recipe-edit__remove-button-container\">\n          <button\n            className=\"btn recipe-edit__remove-button\"\n            onClick={() => handleRecipeSelect(undefined)}\n          >\n            &times;\n          </button>\n        </div>\n        <div className=\"recipe-edit__details-grid\">\n          <label htmlFor=\"name\" className=\"recipe-edit__label\">\n            Name\n          </label>\n          <input\n            className=\"recipe-edit__input\"\n            type=\"text\"\n            name=\"name\"\n            id=\"name\"\n            value={recipe.name}\n            onInput={(e) => handleChange({ name: e.target.value })}\n          />\n          <label className=\"recipe-edit__label\" htmlFor=\"cookTime\">\n            Cook Time\n          </label>\n          <input\n            className=\"recipe-edit__input\"\n            type=\"text\"\n            name=\"cookTime\"\n            id=\"cookTime\"\n            value={recipe.cookTime}\n            onInput={(e) => handleChange({ cookTime: e.target.value })}\n          />\n          <label className=\"recipe-edit__label\" htmlFor=\"servings\">\n            Servings\n          </label>\n          <input\n            className=\"recipe-edit__input\"\n            type=\"number\"\n            min=\"1\"\n            name=\"servings\"\n            id=\"servings\"\n            value={recipe.servings}\n            onInput={(e) =>\n              handleChange({ servings: parseInt(e.target.value) || \"\" })\n            }\n          />\n          <label className=\"recipe-edit__label\" htmlFor=\"instructions\">\n            Instuctions\n          </label>\n          <textarea\n            className=\"recipe-edit__input\"\n            name=\"instructions\"\n            id=\"instructions\"\n            value={recipe.instructions}\n            onInput={(e) => handleChange({ instructions: e.target.value })}\n          />\n        </div>\n        <br />\n        <div className=\"recipe-edit__ingredients\">\n          <label className=\"recipe-edit__label\">Ingredients</label>\n          <div className=\"recipe-edit__ingredient-grid\">\n            <div>Name</div>\n            <div>Amount</div>\n            <div></div>\n            {recipe.ingredients.map((ingredient) => (\n              <RecipeIngredientEdit\n                key={ingredient.id}\n                ingredient={ingredient}\n                handleIngredientChange={handleIngredientChange}\n                handleIngredientDelete={handleIngredientDelete}\n              />\n            ))}\n          </div>\n          <div className=\"recipe-edit__add-ingredient-btn-container\">\n            <button\n              className=\"btn btn--primary\"\n              onClick={() => handleIngredientAdd()}\n            >\n              Add Ingredient\n            </button>\n          </div>\n        </div>\n      </div>\n    </>\n  );\n}\n","/Users/razakhalid/Downloads/my-react-projects/react-recipe-app/src/components/Recipe.js",["54","55"],"/Users/razakhalid/Downloads/my-react-projects/react-recipe-app/src/components/RecipeIngredientEdit.js",[],"/Users/razakhalid/Downloads/my-react-projects/react-recipe-app/src/components/IngredientList.js",[],"/Users/razakhalid/Downloads/my-react-projects/react-recipe-app/src/components/Ingredient.js",[],"/Users/razakhalid/Downloads/my-react-projects/react-recipe-app/src/comp